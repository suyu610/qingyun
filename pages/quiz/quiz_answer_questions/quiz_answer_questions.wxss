.container {
  width: 100vw;
  height: 100vh;
  box-sizing: border-box;
  background: transparent url(https://cdns.qdu.life/img/chat_bg_9.png) no-repeat fixed center;
  background-size: 100vw 100vh;
}

.test {
  box-sizing: border-box !important;
  height: 100%;
}

swiper {
  height: 100%;
}

/* .title {
  width: 100%;
} */

.title .ques_title,
.title .ques_type {
  height: 50rpx;
  line-height: 50rpx;
  display: inline;
}

.title .ques_title {
  font-weight: bold;
}

.title .ques_type {
  padding: 9rpx;
  color: white;
  font-size: 20rpx;
  background-color: #28b1fc;
  margin-right: 10rpx;
  border-radius: 5rpx;
}

.audio_slider {
  width: 90%;
  margin: 0 auto;
}

.title .ques_img {
  padding-top: 20rpx;
  text-align: center;
}

.title .ques_voice {
  padding-top: 40rpx;

}

.title .progress_seconds {
  display: flex;
  justify-content: space-between;
  padding-top: 30rpx;
  font-weight: bold;
}

.title .progress_control {
  display: flex;
  justify-content: center;
  padding-top: 20rpx;
}

.options {
  padding-top: 30rpx;
}

.options .option {
  margin-top: 50rpx;
}

.options .input_class {
  padding-top: 30rpx !important;
}

.ques_list_index_title,
.options .option .option_title {
  width: 50rpx;
  height: 50rpx;
  border: 1px solid #c5c1c1;
  line-height: 50rpx;
  text-align: center;
  border-radius: 50%;
  display: inline-block;
  margin-right: 30rpx;
}

.ques_list_index_title {
  border: 1px solid #b79696;
  color: #b79696;
}

.bg-red {
  background-color: red !important;
  color: white;
  border: 1px solid red;
}

.bg-blue,
.bg-yellow {
  text-align: center;
  border-radius: 50%;
  color: white;
  display: inline-block;
  margin-right: 30rpx;
  background-color: #27B1FF !important;
  border: 1px solid transparent !important;
}

.answer_right {
  background: url(data:svg+xml;base64,);
  width: 40rpx;
  height: 35rpx;
  background-size: contain;
  display: inline-block;
  margin-left: 40rpx;
}

.transition {
  /* filter: blur(2px); */
  opacity: 0;
  transition: .2s;
}

.options .option_multi .option_title {
  border-radius: 15%;
}

.options .option .option_body {
  display: inline;
  font-weight: bold;
}

.footer {
  background-color: #fff;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100vw;
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  border-top: 1px solid #fff4fa;
  font-weight: bold;

}

.footer .answer_btn {
  text-align: left;
  /* text-decoration: underline; */
}

.footer .star .star_num {
  color: black;
}

.footer .do_num .done,
.footer .do_num .undone,
.footer .all_ques,
.footer .star {
  display: flex;
  justify-content: center;
}

.footer .do_num {
  display: flex;
  justify-content: space-around;
}

van-icon {
  padding-right: 10rpx;
}

.quick_opera {
  position: fixed;
  right: 60rpx;
  bottom: 100rpx;
  width: 100rpx;
  height: 490rpx;
  transition: .5s;
}

.quick_opera .opera {
  width: 100%;
  text-align: center;
  margin-top: 33rpx;
  color: lightgrey;
  border-radius: 10rpx;
  border: 1px solid lightgrey;
  height: 64rpx;
  line-height: 64rpx;
  display: block;
}



.tab-active {
  font-weight: bold !important;
}

.answer_page {
  padding: 30rpx;
  padding-top: 20rpx;
  font-size: 24rpx;
  box-sizing: border-box;
  height: 50vh;
}

.answer_page .answer,
.answer_page .others,
.answer_page .mine {
  height: 40vh;
}

z .answer_page .answer .answer_detail {
  text-align: center;
  font-weight: bold;
  padding-top: 20rpx;
}

.answer_page .answer .answer_detail .body {
  padding-top: 30rpx;
  /* color: #28B1FC; */
  font-weight: bold;
  text-align: center;
  font-size: 34rpx;
}

.answer_page .answer .description .author {
  text-align: right;
}

.answer_page .others:first-child {
  padding-top: 30rpx;
}

.answer_page .others .comment .body view {
  display: inline;
}

.answer_page .mine .info,
.answer_page .answer .info,
.answer_page .others .comment .info {
  display: flex;
  justify-content: space-between;
  padding-top: 15rpx;
  color: lightgrey;
}

.answer_page .mine van-icon,
.answer_page .answer van-icon,
.answer_page .others van-icon {
  top: 1px;
  position: relative;
}

.answer_page .eye {
  padding-left: 20rpx;
}

.answer_page .mine .eye {
  padding-right: 20rpx;
}

.answer_page .book-blue {
  color: #28B1FC;
  font-weight: bold;
}

.answer_page .book-grey {
  color: lightgrey;
}

.answer_page .others .comment .body .author {
  /* color: #28B1FC; */
  font-weight: bold;
  padding-right: 20rpx;
}

.answer_page .btns {
  width: 92%;
  box-sizing: border-box;
  position: absolute;
  bottom: 20px;
}

.answer_page .answer_btn {
  color: #fff;
  background-color: #28B1FC;
  border-radius: 20rpx;
  font-weight: bold;
  text-align: center;
}